# mount     fstype      device                  device2     foptions            fstype2     fsoptions2

/boot       bml         /dev/block/bml1
/cache      ext4        /dev/block/stl11        NULL        NULL                rfs         nosuid nodev check=no
/data       ext4        /dev/block/mmcblk0p2    NULL        NULL                rfs         nosuid nodev check=no
/datadata   ext4        /dev/block/stl10        NULL        NULL                rfs         nosuid nodev check=no
/recovery   bml         /dev/block/bml8
/sdcard     vfat        /dev/block/mmcblk1p1    /dev/block/mmcblk1
/system     ext4        /dev/block/stl9         NULL        NULL                rfs         llw,check=no
