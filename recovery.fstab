# mount point	fstype    device    	device2        foptions fstype2 fsoptions2

/boot       bml         /dev/block/bml1
/cache      rfs         /dev/block/stl11        nosuid nodev check=no
/data       rfs         /dev/block/mmcblk0p2    nosuid nodev check=no
/datadata   rfs         /dev/block/stl10        nosuid nodev check=no
/recovery   mtd         /dev/block/bml8         NULL
/sdcard     vfat        /dev/block/mmcblk1p1    /dev/block/mmcblk1
/system     rfs      /dev/block/stl9         llw,check=no
